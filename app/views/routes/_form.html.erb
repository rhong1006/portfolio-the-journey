<%= simple_form_for @route do |f| %>
  <% if @route.errors.present? %>
    <p><%= @route.errors.full_messages.join(', ') %></p>
  <% end %>
  <div class="form-group">
    <%= f.input :duration, class:"form-control" %>
  </div>
  <%= f.submit 'Submit!', class:"btn btn-primary float-right"%>
<% end %>
